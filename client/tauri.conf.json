{
  "build": {
    "beforeDevCommand": "npm run build:PY && npm run dev:App",
    "beforeBuildCommand": "npm run build:PY && npm run build:App",
    "devPath": "http://localhost:3000",
    "distDir": "../src/.output/public",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "ll-dashboard",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true,
        "sidecar": true,
        "scope": [
          {
            "name": "bin/python/main",
            "sidecar": true
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "externalBin": ["bin/python/main"],
      "resources": [
        "bin/python/data"
      ],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.simonilincev.ll-dashboard",
      "targets": "all"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ll-dashboard",
        "width": 800,
        "height": 600
      }
    ]
  }
}
